<html>
<head>
    <meta charset="UTF-8">
    <title>IFE ECMAScript</title>
</head>
<body>
    <script>
      function getPA() {
        let arr = []
        for(let i = 1; i <= 100; i ++) {
          if (i % 3 == 0 || /3/.test(i.toString())) {
            arr.push('PA')
          } else {
            arr.push(i)
          }
        }
        console.log(arr.join(','))
      }
      getPA()
    </script>
</body>
</html>
